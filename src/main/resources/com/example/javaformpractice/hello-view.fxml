<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="437.0" prefWidth="261.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javaformpractice.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Text fx:id="welcomeText" strokeType="OUTSIDE" strokeWidth="0.0" text="Registration Form" />
   <TextField fx:id="FirstName" maxWidth="-Infinity" prefHeight="26.0" prefWidth="169.0" promptText="First Name" />
   <TextField fx:id="LastName" maxWidth="-Infinity" prefHeight="26.0" prefWidth="169.0" promptText="Last Name" />
   <TextField fx:id="Gender" maxWidth="-Infinity" prefHeight="26.0" prefWidth="169.0" promptText="Gender" />
   <TextField fx:id="Age" maxWidth="-Infinity" prefHeight="26.0" prefWidth="169.0" promptText="Age" />
    <Button onAction="#onButtonClick" text="submit" />
</VBox>
